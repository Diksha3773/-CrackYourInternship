find the min diff btween no of chocolates distributed to 'm' students 


****************************
long long findMinDiff(vector<long long> a, long long n, long long m)
    {
 long long min=INT_MAX;
           sort(a.begin(),a.end());
           for(int i=0;i<=(n-m);i++)
           {
               if(a[i+m-1]-a[i]<min)
               {
                   min=a[i+m-1]-a[i];
               }
             }
                     return min;
  }   



****************************
